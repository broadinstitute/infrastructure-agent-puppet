license_key: <%= @license_key %>

proxy: <%= @proxy %>

<% unless @display_name.to_s.strip.empty? -%>
display_name: <%= @display_name %>
<% end -%>

<% if @verbose =~ /[0-1]/  -%>
verbose: <%= @verbose %>
<% end -%>

<% unless @log_file.to_s.strip.empty? -%>
log_file: <%= @log_file %>
<% end -%>

<% if @facter_interval_sec -%>
facter_interval_sec: <%= @facter_interval_sec %>
<% end -%>

<% if @rpm_interval_sec -%>
rpm_interval_sec: <%= @rpm_interval_sec %>
<% end -%>

<% if @selinux_interval_sec -%>
selinux_interval_sec: <%= @selinux_interval_sec %>
<% end -%>

<% if @sysctl_interval_sec -%>
sysctl_interval_sec: <%= @sysctl_interval_sec %>
<% end -%>

<% if @systemd_interval_sec -%>
systemd_interval_sec: <%= @systemd_interval_sec %>
<% end -%>

custom_attributes:
<% @custom_attributes.each do |key,value|-%>
    <%= key %>: <%= value%>
<% end -%>

<%= @custom_configs.to_yaml.sub(/.*?\n/, '') %>
